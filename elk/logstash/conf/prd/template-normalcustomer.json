{
	"index_patterns":["normalcustomer*"],
	"order":10,
	"settings": {
				 "index": {
			"codec": "best_compression",
			"number_of_shards": "3",
			"number_of_replicas": "1",
							"analysis": {
		   "analyzer": {
			  "comma": {
				 "pattern": ",",
				 "type": "pattern"
			  },
			  "ik_max_word": {
				 "tokenizer": "ik_max_word"
			  }
		   }
		}
				 } 
	},
	"mappings": {
		"doc": {
			"properties": {
				"@timestamp" : {  
					"type" : "date",  
					"format" : "yyyy-MM-dd'T'HH:mm:ss.SSS'Z'||dateOptionalTime",  
					"doc_values" : true  
				},
				"pk": {
					"type": "long"
				},
				"name": {
					"type": "text",
					"analyzer": "ik_max_word",
					"search_analyzer": "ik_max_word"
				},
				"code": {
					"type": "keyword"
				},
				"createdts": {
					"type": "date",
					"format": "yyyy-MM-dd'T'HH:mm:ss.SSS'Z'||yyyy-MM-dd HH:mm:ss.SSSSSS||yyyy-MM-dd HH:mm:ss||yyyy-MM-dd||epoch_millis"
				},
				"modifiedts": {
					"type": "date",
					"format": "yyyy-MM-dd'T'HH:mm:ss.SSS'Z'||yyyy-MM-dd HH:mm:ss.SSSSSS||yyyy-MM-dd HH:mm:ss||yyyy-MM-dd||epoch_millis"
				},
				"mobilenumber": {
					"type": "keyword"
				},
				"storelist": {
					"type": "text",
					"analyzer": "comma"
				},
				"storeuidlist": {
					"type": "text",
					"analyzer": "comma"
				},
			    "paperid": {
					"type": "keyword"
				},
				"originaluid": {
					"type": "keyword"
				},
				"type": {
					"type": "long"
				},
				"papertype": {
					"type": "long"
				},
				"typecode": {
					"type": "keyword"
				},
				"typename": {
					"type": "keyword"
				},
				"papertypecode": {
					"type": "keyword"
				},
				"gender": {
					"type": "keyword"
				},
				"multiindex": {
					"type": "text",
					"analyzer": "comma"
				}
			}
		}
	},
	"aliases" : {
        "{index}-alias" : {} 
    }
}
