{
	"order": 11,
	"index_patterns": ["ydstore*"],
	"settings": {
		"index": {
			"codec": "best_compression",
			"number_of_shards": "3",
			"number_of_replicas": "1",
                        "analysis": {
                              "analyzer": {
				  "ik_max_word": {
                                         "tokenizer": "ik_max_word"
                                  }
                              }
                        }
		}
	},
	"mappings": {
		"doc": {
			"properties": {
				"@timestamp" : {  
					"type" : "date",  
					"format" : "yyyy-MM-dd'T'HH:mm:ss.SSS'Z'||dateOptionalTime",  
					"doc_values" : true  
				},
				"pk": {
					"type": "long"
				},
				"name": {
					"type": "text",
					"analyzer": "ik_max_word"
				},
				"uid": {
					"type": "keyword"
				},
				"createdts": {
					"type": "date",
					"format": "yyyy-MM-dd'T'HH:mm:ss.SSS'Z'||yyyy-MM-dd HH:mm:ss.SSSSSS||yyyy-MM-dd HH:mm:ss||yyyy-MM-dd||epoch_millis"
				},
				"modifiedts": {
					"type": "date",
					"format": "yyyy-MM-dd'T'HH:mm:ss.SSS'Z'||yyyy-MM-dd HH:mm:ss.SSSSSS||yyyy-MM-dd HH:mm:ss||yyyy-MM-dd||epoch_millis"
				},
				"oacoding": {
					"type": "keyword"
				},
				"fullname": {
					"type": "text",
					"analyzer": "ik_max_word"
				},
				"formalorderexists": {
					"type": "boolean"
				}
			}
		}
	}
	
}
